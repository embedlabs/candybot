{"title":"Select Account","scrolling":true,"loadflow":"","init":"\n$('body').addClass('textured');$('#header .loading').hide();$('.button').pretouch();if(OF.page.params.mode=='switch'){OF.introSwitch=true;}\nif(OF.introSwitch){$('.switch_flow').unhide(0);$('.intro_flow').hide();}\nvar userTmpl=tmpl('user_tmpl');var html='';$.each(OF.users,function(){html+=userTmpl(this.user);});$('#multiple_user_accounts').hide().html(html).fadeIn();","resume":"","appear":"","html":"<div id='intro'>\n<div id='header'>\n<span class='loading'>\n<span class='spinner'></span>\n</span>\n</div>\n<div class='content'>\n          <script type=\"text/plain\" id=\"user_tmpl\">\n            <div class=\"inset_cell user\" data-href=\"intro/found_device?user_id=<%= id %>\">\n  <div class=\"image avatar\" style=\"<%= (profile_picture_url ? \"background-image:url(\" + profile_picture_url + \")\" : \"\")%>\">\n    <div class=\"overlay\"></div>\n  </div>\n  <div class=\"title user_name\"><%= name %></div>\n  <div class=\"feint_score\"><%= gamer_score %></div>\n</div>\n<div class=\"divider\"></div>\n          </script>\n<p>\nWe've found multiple accounts. Select which one to use or create a new account.\n</p>\n<div id='multiple_user_accounts'></div>\n<div class='button_row clearfix'>\n<div class='button_wrapper clearfix'>\n<div class='button' data-href='intro/new_account'>\nCreate a<br>\nNew Account\n</div>\n</div>\n<div class='button_wrapper clearfix'>\n<div class='button' data-href='intro/login'>\nUse Another<br>\nAccount\n</div>\n</div>\n</div>\n\n</div>\n</div>\n"}